{
  "name": "Taobao",
  "version": "1.0.0",
  "description": "Extension for Taobao",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://s.taobao.com/search?*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_title": "Taobao extension"
  }
}
